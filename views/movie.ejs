<link rel="stylesheet" href="/app.css">
<h1>Search Result Page!!!</h1>
<% if(!searches["Search"] == ""){ %>
<ol>
    <% searches["Search"].forEach(function(movie){ %>
        <li><img width="7%" height="20%" src=<%= movie["Poster"] %> >
        Title of your Movie is: <strong><%= movie["Title"] %></strong> - <%= movie["Year"] %>.</li>
        <br>

<% });  %>
<% } else { %>
        <h2>Please, do not leave the search box blank. or movie cannot be found!!</h2>
<% } %>

</ol>
<button><a href="/">Search again</a></button>